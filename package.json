{
  "name": "@amatias/eslint-config-monorepo",
  "version": "0.0.3-beta.4",
  "private": true,
  "description": "Personal eslint config",
  "author": "Amatias <amatias@foxmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "test": "pnpm -r run test",
    "prepare": "pnpm -r run stub",
    "tryPublish": "pnpm onlyPublish --dry-run",
    "onlyPublish": "pnpm publish -r",
    "bumpp": "bumpp",
    "release": "bumpp -r && pnpm onlyPublish"
  },
  "devDependencies": {
    "@amatias/eslint-config-ts": "*",
    "bumpp": "^9.0.0",
    "eslint": "^8.36.0",
    "rimraf": "^4.4.0",
    "typescript": "^5.0.2"
  },
  "publishConfig": {
    "directory": "packages/*"
  },
  "eslintConfig": {
    "extends": "@amatias/eslint-config-ts"
  }
}